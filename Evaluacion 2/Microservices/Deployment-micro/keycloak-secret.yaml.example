# Keycloak Admin Credentials Template
#
# INSTRUCTIONS:
# 1. Copy: cp keycloak-secret.yaml.example keycloak-secret.yaml
# 2. Update with real credentials
# 3. Don't commit keycloak-secret.yaml to git
#
apiVersion: v1
kind: Secret
metadata:
  name: keycloak-secret
type: Opaque
stringData:
  KEYCLOAK_ADMIN: "admin"
  KEYCLOAK_ADMIN_PASSWORD: "YOUR_PASSWORD_HERE"
