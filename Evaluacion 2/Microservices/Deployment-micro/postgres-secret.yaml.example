apiVersion: v1
kind: Secret
metadata:
  name: postgres-secret
type: Opaque
stringData:
  POSTGRES_USER: "TU_USUARIO_AQUI"
  POSTGRES_PASSWORD: "TU_PASSWORD_AQUI"
